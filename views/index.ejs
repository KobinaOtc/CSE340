<div class="main-div">
    <div>
        <h1>Welcome to CSE Motors!</h1>
    </div>

    <%- messages() %>

    <section class="car-section">
        <aside>
            <h3>DMC Delorean</h3>
            <p>3 Cup holders</p>
            <p>Superman doors</p>
            <p>Fuzzy dice</p>
            <div>
                <button>
                    Own Today
                </button>
            </div>
        </aside>
    </section>

    <section class="car-addons">
        <div class="upgrades-column">
            <h2>Delorean Upgrades</h2>
            <div class="upgrades-grid">
                <%- content %>
            </div>
        </div>
        <div class="reviews-column">
            <h2>DMC Delorean Reviews</h2>
            <ul>
                <%- reviews %>
            </ul>
        </div>
    </section>
</div>